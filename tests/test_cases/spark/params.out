SELECT
  time
FROM UNNEST(SEQUENCE(
  CAST(SUBSTR(CAST(CAST('{{1 - Start Date}}' AS TIMESTAMP) AS VARCHAR), 1, 10) AS DATE),
  LEAST(
    CURRENT_TIMESTAMP,
    CAST(SUBSTR(CAST(CAST('{{2 - End Date}}' AS TIMESTAMP) AS VARCHAR), 1, 10) AS DATE)
  ),
  INTERVAL '1' {{3 - Time Granularity}}
)) AS _u(time)